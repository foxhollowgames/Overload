[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[physics]

2d/default_gravity=20.0

[rendering]

textures/canvas_textures/default_texture_filter=0
global_illumination/size/window_height_override=720
global_illumination/size/window_width_override=1280
global_illumination/stretch/mode="canvas_items"
global_illumination/default_gravity=20.0
